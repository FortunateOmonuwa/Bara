  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],

    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },

    "Enrich": [
      "WithThreadId",
      "WithThreadName",
      "WithProcessId",
      "WithProcessName",
      "WithEnvironmentName",
      "WithMachineName"
    ],

    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Log.md",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Display.MessageTemplateTextFormatter, Serilog",
          "outputTemplate": "## [{Timestamp:yyyy-MM-dd HH:mm:ss}] **[{Level:u3}]**: {Message:lj}\n\n{Exception}\n---\n",
          "closingDelimiter": "\n",
          "renderMessage": true,
          "useSpaces": true,
          "applyAdditionalLevelPadding": true
        }

      },

      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "Bara",
      "Environment": "Development"
    }
